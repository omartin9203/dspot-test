    "amqp-connection-manager": "^3.2.2",
    "amqplib": "^0.7.1",
    "apollo-server-express": "^2.16.1",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "graphql": "^15.3.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-scalars": "^1.4.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^8.3.0",
    "handlebars": "^4.7.6",
